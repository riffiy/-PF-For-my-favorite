<%= form_with(url: timeline_index_path(@outputs_post), remote: true, method: :get, class: "output-search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>

<% @outputs.each do |output| %>
<div>
  <%= output.post %><br>
</div>

<% 
=begin %>
<div id="colections_buttons_<%= output.id %>">
  <%= render 'public/colections/colection', output: output %>
</div>
<% 
=end %>
<% end %>

