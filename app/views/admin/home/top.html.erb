<div class="container text-center">
  
  <h1>Output</h1>
  <%= paginate @outputs %>
  
  <div class="row justify-content-center">
    <% @outputs.each do |output| %>
      <div class="col-7">
        <div class="listbox">
            <p><%= output.post %></p>
            <%= link_to 'Delete', admin_output_path(output.id), method: :delete, class:"link-info", data: {confirm: "削除しますか？"}  %>
        </div>
      </div>
    <% end %>
  </div>
    
</div>